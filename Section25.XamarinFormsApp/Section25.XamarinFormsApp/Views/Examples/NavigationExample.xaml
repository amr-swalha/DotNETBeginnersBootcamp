<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Section25.XamarinFormsApp.Views.Examples.NavigationExample">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Navigation Example"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button x:Name="NavigateSecond" Clicked="NavigateSecond_OnClicked" Text="Go To Second Page"></Button>
        </StackLayout>
        
    </ContentPage.Content>
</ContentPage>